<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_hospital_patient" model="ir.actions.act_window">
        <field name="name">Patients</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">hospital.patient</field>
        <field name="view_mode">tree,form,graph</field>
        <field name="context">{}</field>
    </record>

    <record id="action_hospital_appointment" model="ir.actions.act_window">
        <field name="name">Appointments</field>
        <field name="res_model">hospital.appointments</field>
        <field name="view_mode">tree,form,calendar</field>
        <field name="help" type="html">
            <p>
                will be back!
            </p>
        </field>
    </record>

    <record id="action_hospital_combine" model="ir.actions.act_window">
        <field name="name">Combines</field>
        <field name="res_model">hospital.combine</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p>
                No records found!
            </p>
        </field>
    </record>

    <record id="cafe2.cafe2_tags_action" model="ir.actions.act_window">
        <field name="name">Cafe2 Tags from </field>
        <field name="context">{'default_tag_name':'meow'}</field>
    </record>

</odoo>