<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="patient_appointment_view_tree" model="ir.ui.view">
        <field name="name">patient_appointment.view.tree</field>
        <field name="model">hospital.combine</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="sequence_no" />
                <field name="age" />
                <!-- <field name="patient" /> -->
                <field name="start_date" />
                <field name="date" />
            </tree>
        </field>
    </record>

    <record id="patient_appointment_view_form" model="ir.ui.view">
        <field name="name">patient_appointment.view.form</field>
        <field name="model">hospital.combine</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="sequence_no" />
                        <field name="age" />
                        <field name="patient" widget="Many2one"/>
                        <field name="start_date" />
                        <field name="date" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>